{
    "extends": ["prettier"],
    "plugins": ["prettier", "react"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-unused-vars": ["error"],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
    }
}
